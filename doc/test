        $users_db = $this->getDataManager('users');        
        echo "=============getAll==============\n";
        $ret = $users_db->getAll();
        var_dump($ret);
        echo "============getUserByUid===============\n";
        $ret = $users_db->getUserByUid(2);
        var_dump($ret);
        echo "=============addUser==============\n";
        $ret = $users_db->addUser('ayase');
        var_dump($ret);
        echo "============delete===============\n";
        $ret = $users_db->deleteUser(2);
        var_dump($ret);
        echo "=============updateName==============\n";
        $ret = $users_db->updateUserName(1, 'kamineko');
        var_dump($ret);
        echo "===========updateStatus================\n";
        $ret = $users_db->updateUserStatus(5, 3);
        var_dump($ret);